{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/runtime": ["types/coze-runtime.d.ts"]
    }
  },
  "include": ["*.ts", "types/**/*.ts"],
  "exclude": ["node_modules", "dist"]
} 